    "node-queue-lib": "0.0.28",
    "node-uuid": "^1.4.8",
    "open-ssh-tunnel": "github:SouthbankSoftware/open-ssh-tunnel",
    "openport": "0.0.4",
    "passport": "^0.4.0",
    "pkginfo": "^0.4.1",
    "ps-tree": "^1.1.0",
